{
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron",
      "schedule": "0 12 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/proxy/:match/:url*",
      "destination": "https://:match/:url*"
    },
    {
      "source": "/proxy/:match/:url*/",
      "destination": "https://:match/:url*/"
    }
  ]
}